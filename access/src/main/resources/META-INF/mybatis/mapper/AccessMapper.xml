        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.toast.provider.access.mapper.IAccessMapper">
    <update id="doUpdateToken" parameterType="map">
        UPDATE access SET token=#{token} WHERE aid=#{aid}
    </update>
</mapper>